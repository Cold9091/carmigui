{
  "version": 2,
  "buildCommand": "ls -la node_modules/@vitejs/ && npm run vercel-build",
  "devCommand": "npm run dev",
  "installCommand": "rm -rf node_modules package-lock.json && npm install --force",
  "framework": null,
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/uploads/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/attached_assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
